{
  "_version": "22.0.0",

  "compilerOptions": {
    "lib": ["es2024", "ESNext.Array", "ESNext.Collection", "ESNext.Iterator",],
    "module": "nodenext",
    "target": "es2024",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",

    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "outDir": "dist",
      "allowImportingTsExtensions": true,
    "noEmit": true
  }
}